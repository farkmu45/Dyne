{
  "name": "dyne",
  "version": "1.0.0",
  "description": "Dynamic, flexible website",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:sass": "node-sass sass/main.scss public/css/master.css -w",
    "compile:sass": "node-sass sass/main.scss public/css/master.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/master.comp.css -o public/css/master.prefix.css",
    "compress:css": "node-sass public/css/master.prefix.css public/css/master.css --output-style compressed",
    "start": "node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farkmu45/Dyne.git"
  },
  "keywords": [
    "Web",
    "Testing",
    "Sass",
    "Modern",
    "Node"
  ],
  "author": "Fark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/farkmu45/Dyne/issues"
  },
  "homepage": "https://github.com/farkmu45/Dyne#readme",
  "engines": {
    "node": "10.3.0",
    "npm": "6.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.2",
    "node-sass": "^4.9.0",
    "postcss-cli": "^5.0.0"
  },
  "dependencies": {
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "method-override": "^2.3.10",
    "mongoose": "^5.1.3",
    "musicmatch": "^0.3.2",
    "request": "^2.87.0"
  }
}
