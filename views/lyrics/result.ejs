<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <meta charset="utf-8">
  <title>Result</title>
  <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400" rel="stylesheet">
  <link rel="stylesheet" href="./../css/master.css">
</head>

<body class="container container-lyric">
  <main class="content">
    <section class="content__show content__show--list">
      <% song.message.body.track_list.forEach(function(data) { %>
        <a href="/lyric/<%= data.track.track_id %>" class="content__result">
          <h5 class="content__name content__name--result"><%=data.track.track_name %></h5>
          <p class="content__details"><%=data.track.artist_name %></p>
        </a>
      <% }) %>
    </section>
    <section class="content__powered">
      <h5 class="content__name">Showing result for : <%= query %></h5>
    </section>
  </main>
</body>

</html>
